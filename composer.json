{
    "name": "flexphp/quote-bundle",
    "description": "Quote bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Freddie Gar",
            "email": "freddie.gar@outlook.com"
        }
    ],
    "require": {
        "symfony/http-client": "^5.2",
        "florianv/swap": "^4.3",
        "php-http/curl-client": "^2.2",
        "guzzlehttp/psr7": "^2.1"
    },
    "require-dev": {
        "symfony/dependency-injection": "^5.2",
        "symfony/config": "^5.2",
        "symfony/http-kernel": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "FlexPHP\\Bundle\\QuoteBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlexPHP\\Bundle\\QuoteBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
